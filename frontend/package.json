{
    "name": "frontend",
    "version": "0.3.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "emotion": "9.2.12",
        "emotion-theming": "9.2.9",
        "eslint-config-airbnb": "16.1.0",
        "eslint-plugin-jsx-a11y": "6.1.2",
        "exception-formatter": "1.0.7",
        "history": "4.7.2",
        "install": "0.12.2",
        "lodash": "4.17.14",
        "loglevel": "1.6.1",
        "mineral-ui": "0.54.0",
        "mineral-ui-icons": "0.5.0",
        "npm": "^6.4.1",
        "react": "16.6.3",
        "react-app-polyfill": "0.1.3",
        "react-dom": "16.6.3",
        "react-emotion": "9.2.12",
        "react-loadable": "5.5.0",
        "react-redux": "5.1.1",
        "react-render-html": "0.6.0",
        "react-router-dom": "4.3.1",
        "react-router-tabs": "1.2.0",
        "react-scripts": "^3.0.1",
        "react-toastify": "4.4.3",
        "redux": "4.0.1",
        "redux-catch": "1.3.1",
        "redux-logger": "3.0.6",
        "redux-observable": "1.0.0",
        "redux-persist": "5.10.0",
        "redux-persist-transform-filter": "0.0.18",
        "redux-thunk": "2.3.0",
        "rxjs": "6.3.3",
        "swagger-ui": "^3.23.1",
        "uuid": "3.3.2"
    },
    "scripts": {
        "start": "cross-env HTTPS=true react-scripts start",
        "build": "react-scripts build",
        "analyze": "source-map-explorer build/static/js/main.*",
        "postbuild": "rimraf build/**/*.map",
        "test": "react-scripts test --env=jsdom components/* reducers/* epics/* selectors/* ErrorBoundary/* --reporters=default --reporters=jest-html-reporter",
        "test:unit": "react-scripts test --env=jsdom components/* reducers/* epics/* selectors/* ErrorBoundary/*",
        "test:int": "react-scripts test --env=jsdom integration-tests/*",
        "coverage": "npm test -- --coverage --watchAll=false"
    },
    "jest-html-reporter": {
        "pageTitle": "ZOWE Catalog UI Unit Tests",
        "outputPath": "./test-results/test-report-unit.html",
        "includeFailureMsg": true
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "10.0.3",
        "babel-jest": "^24.8.0",
        "babel-preset-flow": "6.23.0",
        "babel-preset-react-app": "3.1.2",
        "body-parser": "1.18.3",
        "concurrently": "4.1.0",
        "cors": "2.8.5",
        "cross-env": "5.2.0",
        "enzyme": "3.7.0",
        "enzyme-adapter-react-16": "1.7.0",
        "eslint-config-prettier": "3.3.0",
        "eslint-plugin-flowtype": "2.50.3",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-prettier": "2.7.0",
        "eslint-plugin-react": "7.11.1",
        "express": "4.17.3",
        "jest": "24.9.0",
        "jest-environment-enzyme": "^7.0.2",
        "jest-enzyme": "7.0.1",
        "jest-html-reporter": "^2.4.2",
        "jest-junit": "^6.4.0",
        "jest-mock": "23.2.0",
        "nodemon": "1.18.6",
        "prettier": "1.14.2",
        "react-testing-library": "5.3.0",
        "source-map-explorer": "1.6.0",
        "start-server-and-test": "^1.7.11"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/App.{jsx,js}",
            "src/**/*.{jsx,js}",
            "src/**/reducers/*.{jsx,js}",
            "!src/index.js",
            "!src/**/*Container.jsx",
            "!src/responsive-tests/**",
            "!src/e2e/*"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
